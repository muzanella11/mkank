{
  "name": "Mkank",
  "version": "1.0.0",
  "description": "Enem Project",
  "author": "Nurfirliana Muzanella",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ext .js,.vue . --fix",
    "lint:style": "stylelint 'assets/scss/**/*css'",
    "lint:style:fix": "stylelint 'assets/scss/**/*css' --fix",
    "lint": "npm-run-all --parallel lint:js lint:style",
    "lint:fix": "npm-run-all --parallel lint:*:fix",
    "test": "jest",
    "woof": "npm-run-all --parallel lint generate"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "husky": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.0.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "stylelint": "^10.0.0",
    "stylelint-config-standard": "^18.2.0",
    "vue-jest": "^4.0.0-0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn woof"
    }
  }
}
